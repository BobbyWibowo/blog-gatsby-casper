{
  "name": "blog-gatsby-casper",
  "description": "Bobby's personal blog using a Gatsby starter pack that was based on the Casper theme from Ghost.",
  "version": "2.0.0",
  "dependencies": {
    "@mdx-js/mdx": "^1.4.5",
    "@mdx-js/react": "^1.4.5",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.15.14",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^2.1.9",
    "gatsby-plugin-feed": "^2.3.12",
    "gatsby-plugin-lodash": "^3.1.7",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-mdx": "^1.0.41",
    "gatsby-plugin-nprogress": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.6",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-sharp": "^2.2.21",
    "gatsby-plugin-sitemap": "^2.2.12",
    "gatsby-remark-autolink-headers": "^2.1.10",
    "gatsby-remark-copy-linked-files": "^2.1.19",
    "gatsby-remark-embed-video": "^1.7.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.21",
    "gatsby-remark-prismjs": "^3.3.13",
    "gatsby-remark-responsive-iframe": "^2.2.16",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-json": "^2.2.8",
    "gatsby-transformer-remark": "^2.6.22",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.9.0",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.2.1",
    "react-scroll": "^1.7.14",
    "react-share": "^3.0.1",
    "react-social-icons": "^4.1.0",
    "react-twitter-widgets": "^1.7.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "remark-cli": "^7.0.0",
    "remark-preset-lint-recommended": "^3.0.3",
    "standard-version": "^7.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
