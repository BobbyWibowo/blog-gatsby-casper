{
  "name": "blog-gatsby-casper",
  "description": "Bobby's personal blog using a Gatsby starter pack that was based on the Casper theme from Ghost.",
  "version": "2.0.0",
  "dependencies": {
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "@raae/gatsby-remark-oembed": "^0.1.1",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "gatsby": "^2.17.15",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^2.1.17",
    "gatsby-plugin-feed": "^2.3.21",
    "gatsby-plugin-lodash": "^3.1.15",
    "gatsby-plugin-manifest": "^2.2.28",
    "gatsby-plugin-mdx": "^1.0.56",
    "gatsby-plugin-netlify": "^2.1.25",
    "gatsby-plugin-nprogress": "^2.1.14",
    "gatsby-plugin-offline": "^3.0.21",
    "gatsby-plugin-postcss": "^2.1.15",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sharp": "^2.3.0",
    "gatsby-plugin-sitemap": "^2.2.21",
    "gatsby-remark-autolink-headers": "^2.1.18",
    "gatsby-remark-copy-linked-files": "^2.1.30",
    "gatsby-remark-embed-video": "^2.0.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.1.31",
    "gatsby-remark-prismjs": "^3.3.23",
    "gatsby-remark-responsive-iframe": "^2.2.27",
    "gatsby-source-filesystem": "^2.1.37",
    "gatsby-transformer-json": "^2.2.18",
    "gatsby-transformer-remark": "^2.6.35",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "postcss-preset-env": "^6.7.0",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.12.0",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.2.1",
    "react-scroll": "^1.7.14",
    "react-share": "^3.0.1",
    "react-social-icons": "^4.1.0",
    "react-twitter-widgets": "^1.7.1"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.1.1",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.3",
    "standard-version": "^7.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.mdx')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a",
    "full-upgrade": "rm yarn.lock; yarn"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
